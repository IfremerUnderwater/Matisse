<?xml version="1.0" encoding="UTF-8"?>

<mosaic_settings xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../models/parameters/Parameters_VV1.0.xsd">

<fileDescriptor>
</fileDescriptor>

<algo_param>
<sift_code>GPUsift</sift_code>
<scale_factor>1,000</scale_factor>
<maxdist_centers>8,000</maxdist_centers>
<min_matches>12</min_matches>
<max_matches>10000</max_matches>
<First_processed_image>1</First_processed_image>
<Last_processed_image>10</Last_processed_image>
<step_im>1</step_im>

<!--Filtering overlap parameters-->
<filter_overlap>true</filter_overlap>
<max_overlap>0,800</max_overlap>
<min_overlap>0,600</min_overlap>

<!--Bundle adjustment parameters-->
<MaxIter>50</MaxIter>
<max_Roll>0,100</max_Roll>
<max_Pitch>0,100</max_Pitch>
<ba_method>Nav_only</ba_method>
<cosmetic_stretch>false</cosmetic_stretch>

<!--Blending and displaying parameters-->
<expo_comp_method>gain</expo_comp_method>
<seam_method>gc_color</seam_method>
<blending_method>multiband</blending_method>
<no_plot>false</no_plot>
<WriteGeoTiff>true</WriteGeoTiff>
<block_drawing>true</block_drawing>
<block_width>7000</block_width>
<block_height>7000</block_height>

</algo_param>

<cam_param>
<K>1035.26002;0;505.70854;0;1034.57204;474.60786;0;0;1</K>
<V_Pose_C>1.397587;0.448053;-0.069925;0;0;1.570796</V_Pose_C>

</cam_param>

<dataset_param>
<dataset_dir>./</dataset_dir>
<output_dir>./Geotiff_data</output_dir>
<output_filename>MosaicOut.tiff</output_filename>
<navFile>otus.dim2</navFile>
<utm_zone>31</utm_zone>
<utm_hemisphere>N</utm_hemisphere>

</dataset_param>

<vehic_param>
<reproj_std>10,000</reproj_std>
<X_std>5,000</X_std>
<Y_std>5,000</Y_std>
<alt_std>0,800</alt_std>
<Roll_std>0,010</Roll_std>
<Pitch_std>0,010</Pitch_std>
<Yaw_std>0,100</Yaw_std>

</vehic_param>

</mosaic_settings>