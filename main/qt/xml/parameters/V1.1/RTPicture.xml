<?xml version="1.0" encoding="UTF-8"?>

<mosaic_settings xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../models/parameters/Parameters_VV1.1.xsd">

<fileDescriptor>
<modelVersion>V1.1</modelVersion>
<name>RTPicture</name>
<date>01/07/2014</date>
<author></author>
<comments></comments>
</fileDescriptor>

<algo_param>
<sift_code>GPU_sift</sift_code>
<scale_factor>1,000</scale_factor>
<maxdist_centers>10,000</maxdist_centers>
<min_matches>10</min_matches>
<max_matches>inf</max_matches>
<First_processed_image>1</First_processed_image>
<Last_processed_image>inf</Last_processed_image>
<step_im>1</step_im>

<!--Filtering overlap parameters-->
<filter_overlap>false</filter_overlap>
<max_overlap>0,900</max_overlap>
<min_overlap>0,700</min_overlap>

<!--Bundle adjustment parameters-->
<MaxIter>300</MaxIter>
<MaxRoll>inf</MaxRoll>
<max_Pitch>inf</max_Pitch>
<ba_method>Ferrer_2008_with_nav</ba_method>
<cosmetic_strech>true</cosmetic_strech>

<!--Blending and displaying parameters-->
<expo_comp_method>gain</expo_comp_method>
<seam_method>gc_color</seam_method>
<blending_method>multiband</blending_method>
<no_plot>false</no_plot>
<WriteGeoTiff>true</WriteGeoTiff>

</algo_param>

<cam_param>
<K>540.677;0;199.9878;0;539.5830;143.9180;0;0;1</K>
<V_Pose_C>-1.1000;0.2500;-0.1500;0;0;-1.5708</V_Pose_C>

</cam_param>

<dataset_param>
<dataset_dir>./</dataset_dir>
<output_dir>./Geotiff_data</output_dir>
<output_filename>OutputImage.tif</output_filename>
<utm_zone>31</utm_zone>
<utm_hemisphere>N</utm_hemisphere>

</dataset_param>

<dim2_param>
<udp_port>7800</udp_port>

</dim2_param>

<vehic_param>
<reproj_std>1,000</reproj_std>
<X_std>2,000</X_std>
<Y_std>2,000</Y_std>
<alt_std>2,000</alt_std>
<Roll_std>0,010</Roll_std>
<Pitch_std>0,010</Pitch_std>
<Yaw_std>0,010</Yaw_std>

</vehic_param>

<video_input_param>
<idCamera>0</idCamera>
<intervalCapture>1000</intervalCapture>

</video_input_param>

</mosaic_settings>