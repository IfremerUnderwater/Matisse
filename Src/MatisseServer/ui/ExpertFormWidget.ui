/************************/
/*** Generic Widgets ***/
/************************/
QWidget{
   background-color: #434a54;  /* Marine Clair */
}

QWidget:disabled {
   background-color: #252a31;  /* Marine */
}

QLabel
{
  background-color: none;
  border: none;
  font-family: &quot;Montserrat&quot;;
  font-size: 12px;
  font-weight: 700;
  color: #f5f7fa;  /* Gris Clair  */
}

/** QProgressBar **/
QProgressBar {
  border: none;
  background-color: #5c646e; /* Gris fonce  */
}

QProgressBar::chunk {
  background-color: #37bc9b; /* Vert  */
}  

QProgressBar:disabled {
  background-color: #18212E; 

}
QProgressBar::chunk:disabled {
  background-color: #5c646e; 
}

/** QPushButton **/
QPushButton {
  background-color: #434a54;    /*Marine Clair  */
  border: 2px solid #7c828c; /*Gris moyen  */
  font-family: &quot;Montserrat&quot;;
  color: #f5f7fa;  /* Gris Clair  */
  font-size: 12px;
  font-weight: 700;
  text-transform: uppercase;
  min-height: 26px;
}

QPushButton:pressed {
  background-color: #4a89dc; /* Bleu */
  border: none;
}

QPushButton:checked {
  background-color: #4a89dc;  /* Bleu */
  border: none;
}

QPushButton:hover {
   border: 2px solid #4a89dc;  /* Bleu */
}

QPushButton:flat {
     border: 2px solid #4a89dc;  /* Bleu */
}

/** GroupBox **/
QGroupBox {
     background-color: #5c646e; /* Gris fonce  */
     border: none;
    text-transform: uppercase;
    font-family: &quot;Montserrat&quot;;
    font-size: 12px;
    font-weight: bold;
    text-transform: uppercase;
    padding-top: 15;
 }

QGroupBox::title {
    subcontrol-origin: margin;
    subcontrol-position: top center; /* position at the top center */
    padding-top: 2;
    color: rgb(124, 130, 140); /* Gris moyen */
 }

Line {
  color: #7c828c;
  border: none;
}

QGraphicsView{
     background-color: #f5f7fa; 
}


QTabWidget{
    border: 2 px #C4C4C3;
}

QTabWidget::pane {
    border: 2 px solid #444;
    border-radius: 3px;
    padding: 3px;
}

QTabBar::tab {
    background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                stop: 0 #5c646e, stop: 1.0 #7c828c);
    font-family: &quot;Montserrat&quot;;
    font-size: 13px;
    border: 2px solid #C4C4C3;
    border-bottom-color: #C2C7CB; /* same as the pane color */
    border-top-left-radius: 4px;
    border-top-right-radius: 4px;
    min-width: 10ex;
    padding: 2px;
}
